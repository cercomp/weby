<h1><%=  t:list, :param => t('users') %></h1>

<%= will_paginate @users %>

<table class="list">
  <thead>
  <tr>
    <th><%=  'id' %></th>
    <th><%= t:login %></th>
    <th><%=  t:email %></th>
    <th><%= t:status %></th>
    <th><%= t:roles %></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
<% @users.each do |user| %>
  <tr class="<%= cycle 'odd', 'even' %>">
    <td><%=h user.id %></td>
    <td><%=h user.login %></td>
    <td><%=h user.email %></td>
    <td><%= %></td>
    <td> </td>
  </tr>
<% end; reset_cycle %>
  </tbody>
</table>

<%= will_paginate @users %>
